
    <form id="myform" method="post" {{ form_enctype(form) }}>
       
        <div class="row">
            <div class="col-lg-12">
                <label class="control-label">Tipo Conflicto:</label><br/>
                {{form_widget(form.tipoVinculo)}}
            </div>
           
        </div>
        <div class="row">
            <div class="col-lg-12">
                <label class="control-label">Razon Social:</label><br/>
                {{form_widget(form.razonsocial)}}
            </div>
           
        </div>
        <div class="row">
            
            <div class="col-lg-12">
                <label class="control-label">RUC:</label><br/>
                {{form_widget(form.ruc)}}
            </div>    
        </div>
        <div class="row">
           <div class="col-lg-6">
                <label class="control-label">Fecha Inicio:</label><br/>
                {{form_widget(form.fecini)}}
            </div>
            <div class="col-lg-6">
                <label class="control-label">Fecha Fin:</label>
                {{form_widget(form.fecfin)}}
            </div>    
        </div>
        
        
        <div class="row">
            <div class="col-lg-12">
                <label class="control-label">Detalle:</label>
                {{form_widget(form.detalle)}}
            </div>
        </div>
        
    </form>