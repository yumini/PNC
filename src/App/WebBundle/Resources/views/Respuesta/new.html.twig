<form id="myform" method="post">  
        <div class="row">
            <div class="col-lg-12">
                <label class="control-label">Descripción:</label><br/>
                <textarea id="txtRespuesta" class="form-control" rows="5"></textarea>
            </div>
           
        </div>
        <br/>
        <div class="row">
            <div class="col-lg-4">
                <label class="control-label">Puntaje Simbólico:</label><br/>
                <select id="cmbPuntaje" name="cmbAspectoCLave" class="form-control">
                    <option value="+">+</option>
                    <option value="++">++</option>
                    <option value="-">-</option>
                    <option value="--">--</option>
                </select>
            </div>
            <div class="col-lg-8">
            </div>
        </div>
        
      	
</form>
