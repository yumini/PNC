 <div class="list-group">
 {% for entity in pagination %}
<a class="list-group-item" >
    <h4 class="list-group-item-heading">{{entity.nombre}}<small> ({{ entity.getPostulantecontacto().nombre}})</small></h4>
    <p class="list-group-item-text">
		<div class="row" style="font-size:11px;">
				<div class="col-lg-3">
				<label>Cargo:</label><br/>
					{{entity.cargo|default('-')}}
				</div>
				<div class="col-lg-3">
					<label>Telefono:</label><br/>
					{{entity.telefono|default('-')}}
				</div>
				<div class="col-lg-3">
					<label>Fax:</label><br/>
					{{entity.fax|default('-')}}
				</div>
				<div class="col-lg-3">
					<label>Email:</label><br/>
					{{entity.email|default('-')}}
				</div>
        </div>
          				
    </p>
    <button type="button" class="btn btn-info btn-xs" onclick="new OptionButton.Contactos().Edit('{{entity.id}}')"> 
    	<span class="glyphicon glyphicon-edit"></span> Editar
    </button>
     <button type="button" class="btn btn-danger btn-xs" onclick="new OptionButton.Contactos().Delete('{{entity.id}}')"> 
     	<span class="glyphicon glyphicon-remove"></span> Eliminar
     </button>
  </a>
   {% endfor %}
  </div>

 <div>
        {{ knp_pagination_render(pagination,'AppWebBundle:PostulanteContacto:pagination.html.twig') }}
    </div>
