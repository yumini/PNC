<div class="row">
    <div class="col-lg-4">
       <nav class="navbar navbar-default" role="navigation" style="z-index:0">
                    <div class="navbar-header">
                      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex2-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                      </button>
                      <a class="navbar-brand" href="#">Grupos de Evaluaci√≥n</a>
                    </div>
                    <div class="collapse navbar-collapse navbar-ex2-collapse">
                      <button id="btnNewGrupo" type="button" class="btn btn-success navbar-btn pull-right"> <span class="glyphicon glyphicon-plus"> </span></button>
                    </div>
                </nav>



        <div id="list-grupoevaluacion" class="list-group">
            <div  class="well"  style="margin:0px;">
   
         <select class="form-control">
            <option value="0">Todos los concursos</option>

        </select>

        </div>
            {% for entity in pagination %}
                <a href="#" class="list-group-item" data-toggle="popover" title="" data-content="And here's some amazing content. It's very engaging. right?" data-original-title="A Title">             
                    <p class="list-group-item-text" data-id="{{ entity.id }}">
                         <button class="btn btn-danger btn-xs pull-right"  data-id="{{ entity.id }}" data-type="delete">
                            <span class="glyphicon glyphicon-trash"> </span>
                        </button>
                        <span class="pull-right">
                           &nbsp;
                        </span>
                        <button class="btn btn-success btn-xs pull-right"  data-id="{{ entity.id }}" data-type="edit">
                            <span class="glyphicon glyphicon-edit"> </span>
                        </button> 
                        
                       
                        
                        <b>{{entity.nombre}}</b> <br/>
                        <small>{{entity.concurso.nombre}}</small>

                    </p>
                </a>
            {% endfor %}
             <div class="well"  style="margin:0px;">
            {{ knp_pagination_render(pagination,'AppWebBundle:Default:pagination.html.twig') }}
            </div>
        </div>
        
    </div>
    <div class="col-lg-8">
       <ul class="nav nav-tabs" id="myTab">
              <li class="active">
                <a href="#TabEvaluadores" data-toggle="tab">Evaluadores</a>
              </li>
              <li>
                <a href="#TabPostulantes" data-toggle="tab">Postulantes</a>
              </li>
             </ul>

        <div class="tab-content">
          <div class="tab-pane active" id="TabEvaluadores">
                <h4>Listado de Evaluadores</h4>
                <div id="container-evaluadores">
                </div>
            </div>
          <div class="tab-pane" id="TabPostulantes">
                    <h4>Listado de Postulantes</h4>
                    <div id="container-postulantes">
                    </div>
          </div>
         
        </div>
    </div>
    
