<div class="row" style="padding: 5px 5px 0px 5px;margin: 0px 0px;">
    <div class="panel" >
        <div class="panel-heading"> 
            <i class="glyphicon glyphicon-list"></i>
            Grupos de Evaluacion
        </div>
            <div class="row">
                <div class="col-lg-2"  id="container-grupoevaluacion">
                    <!--grupos de evaluacion-->
                </div>
                <div class="col-lg-5">
                    <div class="panel">
                        <div class="panel-heading">
                        <div class="row-fluid">
                            <span class="col-lg-5"><h4>Evaluadores</h4></span>
                            <span class="right">
                      
                                <ul class="list-group-item-text">
                                  <button id="btnNewEvaluador" class="btn btn-success btn-mini" > <i class="glyphicon glyphicon-file"></i> Nuevo</button>
                                </ul>
                             </span>
                        </div>
                        </div>
                    <div id="container-evaluadores">
                    </div>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="panel">
                        <div class="panel-heading">
                        <div class="row-fluid">
                            <span class="col-lg-5"><h4>Postulantes</h4></span>
                            <span class="right">
                      
                                <ul class="list-group-item-text">
                                  <button id="btnNewPostulante" class="btn btn-success btn-mini" > <i class="glyphicon glyphicon-file"></i> Nuevo</button>
                                </ul>
                             </span>
                        </div>
                        </div>
                    <div id="container-postulantes">
                    </div>
                    </div>
                </div>
        </div>
        </div>
</div>
{% block ScriptHead %}
    <script src="{{ asset('js/pages/GrupoEvaluacion/index.js') }}"></script> 
{% endblock %}

<!--plantilla grupo evaluacion-->
<textarea id="template-gruposevaluacion" style="display:none">
    <div id="list-grupoevaluacion" class="list-group">
        plantilla cargada....
    </div> 
</textarea>  

<!--plantilla items grupo evaluacion-->
<textarea id="template-grupoevaluacion-item" style="display:none">
     <p class="list-group-item-text" data-id=<%= entity.id %>>
        <b><%= entity.nombre %></b> - 
        <small><%= entity.descripcion %></small>

    </p>
</textarea>  
{% block windows %}
    <div id="winForm"></div>
{% endblock %}