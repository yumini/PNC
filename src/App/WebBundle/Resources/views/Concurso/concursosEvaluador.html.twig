<h3><b>Listado de Concursos</b></h3>
<div class="list-group">
   {% for entity in pagination %}
           <a href="javascript:return false;" class="list-group-item ">
    <h4 class="list-group-item-heading">{{entity.nombre}} </h4>
    <p class="list-group-item-text">{{entity.presentacion|slice(0,250)|raw}}...</p>
    {% if entity.inscripcionesConcurso|length > 0 %}
    <button type="button" data-type="inscripcion" class="btn btn-danger btn-xs" onclick="new Concurso().Detail({{entity. id}})">
  <span class="glyphicon glyphicon-ok-sign"></span> Inscrito</button>
    {% else %}
    <button type="button" data-type="inscripcion" class="btn btn-success btn-xs" onclick="new Concurso().Detail({{entity.id}})">Ver Detalle</button>
    {% endif %}
    
  </a>
        {% endfor %}
  
  </div>
  <div>
        {{ knp_pagination_render(pagination,'AppWebBundle:Concurso:pagination.html.twig') }}
  </div>
