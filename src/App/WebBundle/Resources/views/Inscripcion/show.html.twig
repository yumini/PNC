{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Inscripcion</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombreproyecto</th>
                <td>{{ entity.nombreproyecto }}</td>
            </tr>
            <tr>
                <th>Nombrecorto</th>
                <td>{{ entity.nombrecorto }}</td>
            </tr>
            <tr>
                <th>Integrantes</th>
                <td>{{ entity.integrantes }}</td>
            </tr>
            <tr>
                <th>Objetivoproyecto</th>
                <td>{{ entity.objetivoproyecto }}</td>
            </tr>
            <tr>
                <th>Fechainiciopy</th>
                <td>{{ entity.fechainiciopy|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechafinpy</th>
                <td>{{ entity.fechafinpy|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Informepostulacionc</th>
                <td>{{ entity.informepostulacionc }}</td>
            </tr>
            <tr>
                <th>Informepostulacionsic</th>
                <td>{{ entity.informepostulacionsic }}</td>
            </tr>
            <tr>
                <th>Terminoaceptacion</th>
                <td>{{ entity.terminoaceptacion }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('inscripcion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('inscripcion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('inscripcion_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
